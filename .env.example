# Ollama Configuration
# 별도 터미널에서 SSH 터널 연결 필요 (Docker 환경):
# ssh -L 11434:127.0.0.1:11434 server@<Mac_IP>
OLLAMA_URL=http://localhost:11434
MODEL_NAME=llama3:8b

# Security
# 허용할 CORS 오리진 (쉼표 구분, * 는 전체 허용)
ALLOWED_ORIGINS=*
# IP당 분당 최대 요청 수
RATE_LIMIT_RPM=60

# Logging
LOG_LEVEL=INFO
